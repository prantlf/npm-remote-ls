    "c8": "^7.12.0",
    "chai": "^4.3.6",
    "nock": "^13.2.9",
    "rc": "^1.2.8",
    "standard": "^17.0.0",
    "standard-version": "^9.5.0",
    "tap": "^12.7.0"
  },
  "dependencies": {
    "async": "^2.6.4",
    "cacheman": "^2.2.1",
    "char-spinner": "^1.0.1",

{
  "name": "@prantlf/npm-remote-ls",
  "version": "2.0.0",
  "description": "Examine a package's dependency graph before you install it",
  "engines": {
    "node": ">= 12.22"
  },
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "lib/index.d.ts",
  "exports": {
    "require": "./dist/index.cjs",
    "import": "./dist/index.mjs"
  },
  "bin": {
    "npm-remote-ls": "./bin/npm-remote-ls.js"
  },
  "files": [
    "bin",
    "dist",
    "lib/index.d.ts"
  ],
  "scripts": {
    "prepare": "rollup -c",
    "lint": "denolint && tsc --noEmit test/exports/types-test.ts",
    "check": "teru-esm test/exports/*.mjs && tap test/*.js",
    "test": "denolint && tsc --noEmit test/exports/types-test.ts && teru-esm test/exports/*.mjs && c8 npx tap test/*.js"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/prantlf/npm-remote-ls.git"
  },
  "keywords": [
    "npm",
    "ls",
    "remote",
    "dependency",
    "dependencies"
  ],
  "author": "Ben Coe <ben@npmjs.com>",
  "contributors": [
    "Ferdinand Prantl <prantlf@gmail.com>"
  ],
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/prantlf/npm-remote-ls/issues"
  },
  "homepage": "https://github.com/prantlf/npm-remote-ls",
  "devDependencies": {
    "@node-rs/deno-lint": "^1.14.0",
    "@semantic-release/changelog": "^6.0.1",
    "@semantic-release/git": "^10.0.1",
    "c8": "^7.12.0",
    "chai": "^4.3.6",
    "nock": "^13.2.9",
    "@prantlf/npm-remote-ls": "link:",
    "rc": "^1.2.8",
    "rollup": "^2.79.0",
    "rollup-plugin-cleanup": "^3.2.1",
    "tap": "^12.7.0",
    "tehanu": "^1.0.1",
    "tehanu-repo-coco": "^1.0.0",
    "tehanu-teru": "^1.0.0",
    "typescript": "^4.8.2"
  },
  "dependencies": {
    "async": "^3.2.4",
    "cacheman": "^2.2.1",
    "char-spinner": "^1.0.1",
    "lodash": "^4.17.21",
    "npm-package-arg": "^9.1.0",
    "once": "^1.4.0",
    "registry-url": "^5.1.0",
    "request": "^2.37.0",
    "semver": "^7.3.7",
    "treeify": "^1.1.0",
    "yargs": "^17.5.1"
  },
  "c8": {
    "check-coverage": true,
    "reporter": [
      "text",
      "lcov"
    ],
    "branches": 100,
    "lines": 100,
    "functions": 100,
    "statements": 100
  },
  "release": {
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/changelog",
      "@semantic-release/npm",
      [
        "@semantic-release/github",
        {
          "failComment": false
        }
      ],
      "@semantic-release/git"
    ]
  },
  "publishConfig": {
    "access": "public"
  }
}
